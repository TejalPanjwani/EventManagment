<div class="container">
  <div class="jumbotron">

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>Title:</strong></h4>
        </div>
        <div class="col-sm-10">        
            <h4><%= @title.title_name %></h4>
        </div>
    </div>
    <br><br>

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>Nameofservice:</strong></h4>
        </div>
        <div class="col-sm-10">        
            <h4><%= @service.nameofsevice %></h4>
        </div>
    </div>
    <br><br>

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>City:</strong></h4>
        </div>
        <div class="col-sm-10">     
   
            <h4><%= @city.city_name %></h4>
        </div>
    </div>
    <br><br>

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>Address:</strong></h4>
        </div>
        <div class="col-sm-10">     
   
            <h4><%=@service.address %></h4>
        </div>
    </div>
    <br><br>

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>Contact:</strong></h4>
        </div>
        <div class="col-sm-10">     
   
            <h4><%= @service.contact  %></h4>
        </div>
    </div>
    <br><br>
    
    <p>
        <strong>image</strong>
        <%= image_tag(@service.service_image) if @service.service_image.attached?%>

    </p>

    <div class="form-group">
        <div class="control-label col-md-2">
            <h4><strong>Description:</strong></h4>
        </div>
        <div class="col-sm-10">     

            <h4><%=  @service.description %></h4>
        </div>
    </div>
    <br><br><br><br>

  </div>
</div>